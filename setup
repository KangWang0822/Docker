#Download images: https://hub.docker.com/
#phylogicndt: https://hub.docker.com/r/mxhe/phylogicndt/tags
docker pull mxhe/phylogicndt:upstream_ab72acf
###check images available
docker -images
###Run images
docker run -i mxhe/phylogicndt:upstream_ab72acf
###enter the docker and construct a folder for bind mount 
mkdir results
#sharing the folder between docker and local folder
# on Mac
cd /Users/kangwang/KI/Bioinfo_tools/Docker
docker run -it -v "$(pwd)":/results mxhe/phylogicndt:upstream_ab72acf sh  ###results would be constructed on Docker/images
